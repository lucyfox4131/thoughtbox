<div class="container">
  <div class="col-md-6">

    <h1>Sign Up</h1>

    <%= form_for(@user) do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>
    <%= f.submit "Create Account" %>
    <% end %>

  </div>

  <div class="col-md-6">
    
  </div>
</div>
